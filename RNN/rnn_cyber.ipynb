import pandas as pd
import random
import torch
import torch.nn as nn
from torch.utils.data import Dataset, DataLoader
from torch.nn.utils.rnn import pad_sequence

# -----------------------------
# 1) Define events for scenarios
# -----------------------------
benign_events = [
    "login_success", "open_file", "save_file", "logout",
    "send_email", "receive_email", "update_completed", "backup_completed"
]

brute_force_events = ["failed_login"] * 3 + ["login_success"]
file_exfiltration_events = ["open_file", "copy_file", "external_connection", "logout"]
malware_events = ["open_registry", "write_registry", "execute_file", "delete_logs", "create_process"]
phishing_events = ["email_received", "link_clicked", "credentials_entered"]
ddos_events = ["conn_request"] * 5 + ["timeout"]

# -----------------------------
# 2) Generate dataset
# -----------------------------
data = []
sequence_length = 5

# Benign sequences
for _ in range(100):
    seq = random.choices(benign_events, k=sequence_length)
    data.append([" ".join(seq), 0])

# Malicious sequences (20 each type)
for _ in range(20):
    data.append([" ".join(random.choices(brute_force_events, k=sequence_length)), 1])
    data.append([" ".join(random.choices(file_exfiltration_events, k=sequence_length)), 1])
    data.append([" ".join(random.choices(malware_events, k=sequence_length)), 1])
    data.append([" ".join(random.choices(phishing_events, k=sequence_length)), 1])
    data.append([" ".join(random.choices(ddos_events, k=sequence_length)), 1])

# Shuffle and save
random.shuffle(data)
df = pd.DataFrame(data, columns=["sequence", "label"])
df.to_csv("cybersecurity_sequences.csv", index=False)
print("Dataset created and saved as 'cybersecurity_sequences.csv'")
print(df.head())

# -----------------------------
# 3) Load dataset & tokenize
# -----------------------------
df = pd.read_csv("cybersecurity_sequences.csv")
sequences = df['sequence'].tolist()
labels = df['label'].tolist()

word2idx = {"<pad>":0, "<unk>":1}
tokenized = []

for seq in sequences:
    tokens = seq.split()
    tokenized.append([word2idx.setdefault(w, len(word2idx)) for w in tokens])

# -----------------------------
# 4) Dataset & DataLoader
# -----------------------------
class SeqDataset(Dataset):
    def __init__(self, sequences, labels):
        self.sequences = sequences
        self.labels = labels
    def __len__(self):
        return len(self.sequences)
    def __getitem__(self, idx):
        return torch.tensor(self.sequences[idx]), torch.tensor(self.labels[idx])

def collate_fn(batch):
    seqs = [item[0] for item in batch]
    labs = [item[1] for item in batch]
    padded = pad_sequence(seqs, batch_first=True)
    return padded, torch.tensor(labs)

dataset = SeqDataset(tokenized, labels)
train_loader = DataLoader(dataset, batch_size=16, shuffle=True, collate_fn=collate_fn)

# -----------------------------
# 5) Define RNN model
# -----------------------------
device = torch.device("cuda" if torch.cuda.is_available() else "cpu")

class CyberRNN(nn.Module):
    def __init__(self, vocab_size, embed_dim=16, hidden_dim=32):
        super().__init__()
        self.embed = nn.Embedding(vocab_size, embed_dim, padding_idx=0)
        self.rnn = nn.GRU(embed_dim, hidden_dim, batch_first=True)
        self.fc = nn.Linear(hidden_dim, 1)
        self.sigmoid = nn.Sigmoid()
    def forward(self, x):
        x = self.embed(x)
        _, h = self.rnn(x)
        h = h.squeeze(0)
        out = self.fc(h)
        return self.sigmoid(out).squeeze()

model = CyberRNN(len(word2idx)).to(device)
criterion = nn.BCELoss()
optimizer = torch.optim.Adam(model.parameters(), lr=0.01)

# -----------------------------
# 6) Train RNN
# -----------------------------
epochs = 10
for epoch in range(epochs):
    model.train()
    total_loss = 0
    for seqs, labs in train_loader:
        seqs, labs = seqs.to(device), labs.float().to(device)
        optimizer.zero_grad()
        preds = model(seqs)
        loss = criterion(preds, labs)
        loss.backward()
        optimizer.step()
        total_loss += loss.item()
    print(f"Epoch {epoch+1}/{epochs}, Loss: {total_loss/len(train_loader):.4f}")

# -----------------------------
# 7) Test on new sequences
# -----------------------------
def predict_sequence(seq):
    model.eval()
    tokens = [word2idx.get(w,1) for w in seq.split()]
    tensor = torch.tensor(tokens).unsqueeze(0).to(device)
    with torch.no_grad():
        score = model(tensor).item()
    return score

new_sequences = [
    "failed_login failed_login failed_login login_success backup_completed",
    "login_success open_file save_file logout backup_completed",
    "open_registry write_registry execute_file delete_logs create_process",
    "conn_request conn_request conn_request conn_request timeout",
    "email_received link_clicked credentials_entered login_success open_file"
]

for seq in new_sequences:
    score = predict_sequence(seq)
    label = "Malicious" if score>=0.5 else "Benign"
    print(f"Sequence: {seq}\nScore: {score:.4f}, Predicted: {label}\n")

